<div class="container">
  <div class="row">
    <h3 class="wp">Users</h3>
    <div class="column">
      <%= link_to 'Add New User', new_user_path %>
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
